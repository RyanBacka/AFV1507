var mainWin=Ti.UI.createWindow({backgroundColor:"#f0f8ff",layout:"vertical"}),title=Ti.UI.createLabel({text:"Weather App",top:"20dp"});mainWin.add(title);var buildGeoUi=function(e,t){console.log(e,t),mainWin.add(Ti.UI.createLabel({text:"Latitude: "+e,top:"50dp"})),mainWin.add(Ti.UI.createLabel({text:"Longitude: "+t,top:"70dp"}))},buildUi=function(e){locLbl=Ti.UI.createLabel({text:e.location.city+", "+e.location.state+" ("+e.location.zip+")",top:"10dp",left:"40dp"}),mainWin.add(locLbl);var t=Ti.UI.createView({top:"10dp",left:"20dp",width:"90%",height:"215dp",backgroundColor:"#fff"});mainWin.add(t);var o=Ti.UI.createLabel({text:"Current Weather: ",top:"20dp",left:"20dp"});t.add(o);var a=Ti.UI.createLabel({text:e.current.weather,top:"110dp",left:"60dp"});t.add(a);var r=Ti.UI.createLabel({text:"Temperature: "+e.current.temperature_string,top:"140dp",left:"60dp"});t.add(r);var i=Ti.UI.createImageView({image:e.current.icon_url,top:"60dp",left:"90dp"});t.add(i);var n=Ti.UI.createLabel({text:"Wind: "+e.current.wind_string,top:"170dp",left:"60dp"});t.add(n);var d=Ti.UI.createView({top:"20dp",left:"20dp",width:"90%",height:"320dp",layout:"vertical",backgroundColor:"#fff"});for(mainWin.add(d),x=0;3>x;x++)d.add(Ti.UI.createLabel({text:e.forecast.forecastday[x].title,left:"20dp",top:"10dp"})),d.add(Ti.UI.createLabel({text:e.forecast.forecastday[x].fcttext,left:"30dp",top:"10dp"}))};mainWin.open(),exports.buildUi=buildUi,exports.buildGeoUi=buildGeoUi;